{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": "./public/js/",
      "includePattern": ".js$",
      "excludePattern": "(node_modules/)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "./public/docs/",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}